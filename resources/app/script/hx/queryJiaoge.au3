BlockInput(1)
Global $main = WinActivate("网上股票交易系统5.0")
WinSetState($main, "", @SW_SHOW)
queryJiaoge($CmdLine[1], $CmdLine[2])
WinSetState($main, "", @SW_HIDE)
BlockInput(0)

Func queryJiaoge($startDate, $endDate)
   ControlClick($main, "", "[CLASS:SysTreeView32; INSTANCE:1]", "left", 1, 77, 335)
   Sleep(500)
   Dim $start = ControlGetHandle($main, "", "[CLASS:SysDateTimePick32; INSTANCE:1]")
   setDate($start, $startDate)
   Dim $end = ControlGetHandle($main, "", "[CLASS:SysDateTimePick32; INSTANCE:2]")
   setDate($end, $endDate)
   ControlClick($main, "", "确定")

   Sleep(3000)
   ControlClick($main, "", "[CLASS:CVirtualGridCtrl; INSTANCE:4]")
   Sleep(100)
   Send("^c")
   Sleep(100)
   Dim $text = ClipGet()
   ConsoleWrite($text)
EndFunc

Func setDate($ctrl, $date)
   Dim $time = StringSplit($date, "-")
   ControlClick($main, "", $ctrl, "left", 1, 20, 12)
   Send($time[1])
   Sleep(200)
   ControlClick($main, "", $ctrl, "left", 1, 40, 12)
   Send($time[2])
   Sleep(200)
   ControlClick($main, "", $ctrl, "left", 1, 60, 12)
   Send($time[3])
   Sleep(200)
EndFunc